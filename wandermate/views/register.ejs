    <title>Login</title>
    <link rel="stylesheet" href="/signup/style.css">
  </head>
  <body>
    <br><br><br>

<form method="post"  enctype="multipart/form-data">

<% include ./partials/messages.ejs %>
  <div class="container register">
                  <div class="row">
                      <div class="col-md-3 register-left">
                        <div id="profile-container">
<image id="profileImage" src="data:image/png;base64," />

</div>
<input id="imageUpload" type="file"
name="userPhoto" placeholder="Photo" required="" capture>
                          <!-- <img src="https://image.ibb.co/n7oTvU/logo_white.png" alt=""/> -->
                          <!-- <input type="file" name="userPhoto" value="fileupload" id="fileupload"> -->

                          <h3>Welcome</h3>
                          <p>You are 30 seconds away from a memorable trip</p>
                          <a href="/users/login"><input type="button" name="" value="Login"/></a><br/>
                      </div>
                      <div class="col-md-9 register-right">
                          <div class="tab-content" id="myTabContent">
                              <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                                  <h3 class="register-heading">Register as User</h3>
                                  <div class="row register-form">
                                      <div class="col-md-6">
                                          <div class="form-group">
                                            <input type="text" class="form-control" name="name" placeholder="Name" value="<%= typeof name != 'undefined' ? name:'' %>"   required />
                                          </div>
                                          <div class="form-group">
                                            <input type="text" class="form-control" name="username" placeholder="Username" value="<%= typeof username != 'undefined' ? username:'' %>" required />
                                          </div>
                                          <div class="form-group">
                                            <input type="password" class="form-control" name="password" placeholder="Password" suggested= "new-password" value="<%= typeof password != 'undefined' ? password:'' %>" required />
                                          </div>
                                          <div class="form-group">
                                            <input type="password" class="form-control" name="password1" placeholder="Confirm Password" suggested= "new-password" value="<%= typeof password1 != 'undefined' ? password1:'' %>" required />
                                          </div>
                                      </div>
                                      <div class="col-md-6">
                                          <div class="form-group">
                                            <input type="email" name="email" class="form-control" placeholder="Email" value="<%= typeof email != 'undefined' ? email:'' %>" required />
                                          </div>
                                          <div class="form-group">
                                            <input type="text" name="phone_number" class="form-control" placeholder="Phone Number" required value="<%= typeof phone_number != 'undefined' ? phone_number:'' %>" />
                                          </div>
                                          <div class="form-group">
                                            <input type="text" name="city"  class="form-control" placeholder="City" value="<%= typeof city != 'undefined' ? city:'' %>" required />
                                          </div>
                                          <div class="form-group">
                                            <input type="text" name="state" class="form-control" placeholder="State" value="<%= typeof state != 'undefined' ? state:'' %>" required />

                                          </div>

                                          <input type="submit" class="btnRegister"  name='button' value="Register"/>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>

              </div>
